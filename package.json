{
  "name": "pvtlsalientchild",
  "version": "0.1.0",
  "description": "PVTL Child Theme for the Salient Theme: https://themeforest.net/item/salient-responsive-multipurpose-theme/4363266",
  "license": "MIT",
  "main": "webpack.mix.js",
  "scripts": {
    "install": "npm run production",
    "dev": "npm run development",
